generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model usuario {
  id        String  @id
  nome      String  @unique
  email     String
  senha     String
  permissao String  @default("c")
  ativo     Boolean @default(true)
}
