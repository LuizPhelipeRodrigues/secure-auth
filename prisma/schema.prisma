generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model usuario {
  id        String @id @default(uuid())
  nome      String
  email     String
  senha     String
  permissao String @default("c")
}
